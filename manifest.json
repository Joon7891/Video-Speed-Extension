{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "permissions": ["storage"],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
        "matches": [
        "<all_urls>"
        ],
        "js": ["jquery-3.5.1.min.js", "content.js"]
    }
    ]
}
